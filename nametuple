from collections import namedtuple
import math

Dot = namedtuple('Dot', 'x y')
p1, p2 = Dot(0,0), Dot(0, 5)

def distance(p1, p2):
    x_distance = math.pow((p1.x - p2.x), 2)
    y_distance = math.pow((p1.y - p2.y), 2)
    return math.sqrt((x_distance + y_distance))

print(p1)
print(p2)
print(distance(p1, p2))
